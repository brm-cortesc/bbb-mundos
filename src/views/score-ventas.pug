//-Layout Base
extends ../templates/base.pug

//-Etiqueta Title
block title
	-var titulo = "Ventas";
	title BON BON BUM Cereza Limón

//-Etiqueta Metadescription
block description
	meta(name="description", content="BON BON BUM Cereza Limón" )


block content
	section.container-fluid.container-score
		div.row

		div.container-divider
			div.divider.divider-left
				div.divider-score
					h2
						+image('los-dulces.svg', 'Los Dulces')

					h3.score 9999
					p puntos


			div.divider.divider-right
				div.divider-score
					h2
						+image('los-acidos.svg', 'Los Ácidos')

					h3.score 9999
					p puntos





block Scripts
	script(src="js/bbb-mundos.resultados.js")
	//-Acá se ponen js que solo vayan en ésta página
